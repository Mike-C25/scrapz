<h1>Find Weather Quick!</h1>


<div class="weather-container">
	{{#each weather}}
		{{> main/weather}}
	{{/each}}
</div>

<div class="search-container">
	<div class="description">
		<p>Put in Valid Zip Code to search 10-day weather!</p>
	</div>
	<input id="zip" type="text" name="zipcode">
	<div class="submit-btn">Search</div>
</div>
